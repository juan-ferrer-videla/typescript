{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2022",
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": true,
    "outDir": "./dist"
  },
  "include": ["./src"]
}
